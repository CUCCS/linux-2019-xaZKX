{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":false,
      "action":"accept",
      "matcher":"localhost"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":true,
      "code":"404",
      "response":"rule_ip_reject",
      "matcher":"rule_ip_request",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"404",
      "response":"rule_wp_reject",
      "matcher":"wp_json",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"404",
      "response":"dvwa_sql_response",
      "matcher":"dvwa_sql_injection",
      "custom_response":true,
      "action":"block"
    },{
      "enable":false,
      "code":"404",
      "response":"rule_dvwa_reject",
      "matcher":"rule_dvwa_whitelist",
      "custom_response":true,
      "action":"block"
    },{
      "enable":false,
      "code":"404",
      "response":"vn_whitelist_reject",
      "matcher":"vn_whitelist",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"404",
      "response":"curl_reject",
      "matcher":"forbid_curl",
      "custom_response":true,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"dvwa",
      "upstream":"dvwa",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wordpress",
      "proxy_host":""
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "frequency":{
      "content_type":"text/html",
      "body":"超过访问频率限制！"
    },
    "dvwa_sql_response":{
      "content_type":"text/html",
      "body":"SQL注入不合法"
    },
    "rule_dvwa_reject":{
      "content_type":"text/html",
      "body":"该用户IP不允许访问DVWA！"
    },
    "rule_ip_reject":{
      "content_type":"text/html",
      "body":"不支持IP访问本站点，请使用域名访问！"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    },
    "rule_wp_reject":{
      "content_type":"text/html",
      "body":"漏洞已修复！"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "curl_reject":{
      "content_type":"text/html",
      "body":"禁止curl访问"
    },
    "vn_whitelist_reject":{
      "content_type":"text/html",
      "body":"仅允许白名单用户访问"
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "wordpress":{
      "node":{
        "wp":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"8080"
        }
      },
      "method":"random"
    },
    "dvwa":{
      "node":{
        "dvwa":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"9080"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "rule_dvwa_whitelist":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"≈"
      },
      "IP":{
        "value":"192.168.56.109",
        "operator":"!="
      }
    },
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "forbid_curl":{
      "UserAgent":{
        "value":"curl",
        "operator":"≈"
      }
    },
    "rule_ip_request":{
      "Host":{
        "value":"192\\.168\\.56\\.109.*",
        "operator":"≈"
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    },
    "vn_whitelist":{
      "URI":{
        "value":"^/verynginx/index.html",
        "operator":"≈"
      },
      "IP":{
        "value":"127.0.0.1",
        "operator":"!="
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "wordpress":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "dvwa_sql_injection":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      },
      "Args":{
        "name_operator":"=",
        "name_value":"id",
        "operator":"≈",
        "value":"select|union|or|order by"
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "all_request":{
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "wp_json":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      },
      "URI":{
        "value":"^/wp-json/wp/v2/users.*",
        "operator":"≈"
      }
    },
    "localhost":{
      "IP":{
        "operator":"=",
        "value":"127.0.0.1"
      }
    }
  },
  "static_file_enable":true,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "separate":["ip"],
      "code":"404",
      "response":"frequency",
      "count":"50",
      "matcher":"dvwa",
      "custom_response":true,
      "time":"1"
    },{
      "enable":true,
      "separate":["ip"],
      "code":"404",
      "response":"frequency",
      "count":"20",
      "matcher":"wordpress",
      "custom_response":true,
      "time":"1"
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[]
}